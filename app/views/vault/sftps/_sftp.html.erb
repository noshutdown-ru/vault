<tr class='key <%= defined?(parity) ? parity : '' %>'>
  <td class='checkbox hide-when-print'>
    <%= check_box_tag('ids[]', sftp.id, false, id: nil) %>
  </td>
  <%= render partial: 'shared/key_fields', locals: {key: sftp} %>
  <td class='key-password-column'>
    <% unless sftp.body.blank? %>
      <a class='keys-actions copy-key' data-clipboard-target='copy_body_<%=sftp.id%>' title='<%=t('key.btn.clipboard')%>'>
        <i class="fa fa-clipboard fa-fw"></i>
      </a>
      <label>*********</label>
      <input style="position: absolute; left: -9999px; top: -9999px;" type="text" id="copy_body_<%=sftp.id%>" value="<%=sftp.body.force_encoding('UTF-8')%>" readonly />
    <% end %>
  </td>
  <td class='buttons'>
    <%= render partial: 'shared/key_actions', locals: {key: sftp} %>
  </td>
</tr>
